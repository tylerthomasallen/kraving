<h1>new link page</h1>

<h1>New Link</h1>

<form action="<%= links_url %>" method="post">
  <%= auth_token_input %>

  <label>
    Title
    <input type="text" name="link[title]" value="<%= @link.title %>" id="title">
  </label>

  <label>
    URL
    <input type="text" name="link[url]" value="<%= @link.url %>" id="url ">
  </label>

<input type="submit" name="" value="Create New Link">

</form>
